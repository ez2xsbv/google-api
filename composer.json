{
    "name": "ez2xs/google-api",
    "type": "library",
    "description": "EZ2XS Google client library for Google APIs",
    "keywords": ["EZ2XS", "google"],
    "homepage": "https://github.com/ez2xsbv/google-api",
    "license": "Apache-2.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ez2xsbv/google-api.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "google/auth": "^1.10",
        "ez2xs/google-api-services": "~0.13",
        "firebase/php-jwt": "~2.0||~3.0||~4.0||~5.0",
        "monolog/monolog": "^1.17|^2.0",
        "phpseclib/phpseclib": "~0.3.10||~2.0",
        "guzzlehttp/guzzle": "~5.3.1||~6.0||~7.0",
        "guzzlehttp/psr7": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36|^5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "symfony/dom-crawler": "~2.1",
        "symfony/css-selector": "~2.1",
        "cache/filesystem-adapter": "^0.3.2",
        "phpcompatibility/php-compatibility": "^9.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "composer/composer": "^1.10"
    },
    "suggest": {
        "cache/filesystem-adapter": "For caching certs and tokens (using Google_Client::setCache)"
    },
    "autoload": {
        "psr-0": {
            "Google_": "src/"
        },
        "classmap": [
            "src/Google/Service/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
